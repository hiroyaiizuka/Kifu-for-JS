{
  "name": "kifu-for-js",
  "version": "3.0.1",
  "description": "Shogi Kifu viewer for JavaScript",
  "main": "bundle/kifu-for-js.min.js",
  "types": "bundle/src/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "webpack-dev-server --open-page examples/example.html",
    "build": "NODE_ENV=production webpack -p --env.production",
    "build:ghpages": "webpack -p --env.production --env.ghpages",
    "build:analyze": "webpack -p --env.production --env.analyze",
    "build:watch": "webpack --watch",
    "lint": "tslint -p ./",
    "lint:fix": "prettier ./src/** --write && tslint -p ./ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "deploy:ghpages": "rm -rf ./public && mkdir -p ./public/src && mkdir -p ./public/out & npm run build:ghpages",
    "postdeploy:ghpages": "mv ./bundle/public-bookmarklet.min.js ./public/src/ && mv ./bundle/KifuInBookmarklet.min.js ./bundle/vendors~KifuInBookmarklet.min.js ./public/out/ && cp -R examples ./public && mv bundle readme-ss public/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/na2hiro/Kifu-for-JS.git"
  },
  "keywords": [
    "shogi",
    "kifu",
    "viewer",
    "player",
    "kifuforjs",
    "jkf"
  ],
  "author": "na2hiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/na2hiro/Kifu-for-JS/issues"
  },
  "homepage": "https://github.com/na2hiro/Kifu-for-JS#readme",
  "dependencies": {
    "cypress": "^6.5.0",
    "fscreen": "^1.1.0",
    "json-kifu-format": "^1.2.7",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "react": "^16.0.0",
    "react-dnd": "^7.7.0",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dnd-multi-backend": "^3.0.4",
    "react-dnd-touch-backend": "^0.8.3",
    "react-dom": "^16.0.0",
    "react-responsive": "^8.2.0",
    "shogi.js": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.1.0",
    "@types/react-dnd": "^2.0.36",
    "@types/react-dnd-html5-backend": "^2.1.9",
    "@types/react-dnd-multi-backend": "^3.0.1",
    "@types/react-dom": "^16.0.4",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "img-loader": "^3.0.1",
    "jest": "^26.1.0",
    "jquery": "^3.3.1",
    "mobx-react-devtools": "^5.0.1",
    "pre-push": "^0.1.1",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^0.20.3",
    "ts-jest": "^26.1.3",
    "ts-loader": "^4.5.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.1.2"
  },
  "pre-push": [
    "lint",
    "test"
  ]
}
